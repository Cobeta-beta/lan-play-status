    "he": "^1.2.0",
    "jest": "^27.0.5",
    "vue-cli-plugin-i18n": "^2.3.2",
    "prettier": "^1.19.1",
    "sass": "^1.69.0",
    "sass-loader": "^10",
    "sitemap-webpack-plugin": "^1.1.0",
    "svgo": "^2.2.2",
    "xml2js": "^0.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": 0
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
